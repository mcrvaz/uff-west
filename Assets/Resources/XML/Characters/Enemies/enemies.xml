            <phase>Beginning</phase>
            <sequential>true</sequential>
        </Dialog>
        <Dialog>
            <text>I can't wait to collect your head</text>
            <source>Enemy</source>
            <phase>Beginning</phase>
            <sequential>false</sequential>
        </Dialog>
        
        <Dialog>
            <text>Time for a choppin block party!</text>
            <source>Enemy</source>
            <phase>Defeat</phase>
            <sequential>false</sequential>
        </Dialog>

      </Dialogs>
    </Enemy>
    
	<Enemy characterName="Padre">
