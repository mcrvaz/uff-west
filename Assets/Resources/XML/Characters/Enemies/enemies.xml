    <Enemy characterName="Boss">
      <health>100</health>
      <damage>5</damage>
      <prefab>Boss</prefab>
      <minTimeToClick>0.2</minTimeToClick>
      <maxTimeToClick>0.5</maxTimeToClick>
      <Dialogs>
        <Dialog>
            <text>I salute you</text>
            <source>Enemy</source>
            <phase>Beginning</phase>
            <sequential>true</sequential>
        </Dialog>
        <Dialog>
            <text>For coming this far</text>
