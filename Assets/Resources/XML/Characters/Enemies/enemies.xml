<?xml version="1.0" encoding="Windows-1252"?>
<EnemyCollection xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <Enemies>
  
    <Enemy characterName="Blue Enemy">
      <health>100</health>
      <damage>5</damage>
      <prefab>Enemy</prefab>
      <minTimeToClick>0.3</minTimeToClick>
      <maxTimeToClick>0.6</maxTimeToClick>
      <Dialogs>
      
        <Dialog>
            <text>Mama, I just killed a man</text>
            <source>Enemy</source>
            <phase>Beginning</phase>
            <sequential>true</sequential>
        </Dialog>
        <Dialog>
            <text>Put a gun against his head</text>
            <source>Enemy</source>
            <phase>Beginning</phase>
            <sequential>true</sequential>
        </Dialog>
        <Dialog>
            <text>Pulled my trigger, now he's dead</text>
            <source>Enemy</source>
            <phase>Beginning</phase>
            <sequential>false</sequential>
        </Dialog>
        
        <Dialog>
            <text>Oh, mama!</text>
            <source>Enemy</source>
            <phase>Victory</phase>
            <sequential>true</sequential>
        </Dialog>
        <Dialog>
            <text>I don't wanna die</text>
            <source>Enemy</source>
            <phase>Victory</phase>
            <sequential>true</sequential>
        </Dialog>
        <Dialog>
            <text>I wish I'd never been born at all</text>
            <source>Enemy</source>
            <phase>Victory</phase>
            <sequential>false</sequential>
        </Dialog>
        
        <Dialog>
            <text>Easy come</text>
            <source>Enemy</source>
            <phase>Defeat</phase>
            <sequential>true</sequential>
        </Dialog>
        <Dialog>
            <text>Easy go</text>
            <source>Enemy</source>
            <phase>Defeat</phase>
            <sequential>false</sequential>
        </Dialog>
        
      </Dialogs>
    </Enemy>

    <Enemy characterName="OldGuy">
      <health>100</health>
      <damage>5</damage>
      <prefab>OldGuy</prefab>
      <minTimeToClick>0.3</minTimeToClick>
      <maxTimeToClick>0.6</maxTimeToClick>
      <Dialogs>
      
        <Dialog>
            <text>Have you ever heard any stories</text>
            <source>Enemy</source>
            <phase>Beginning</phase>
            <sequential>true</sequential>
        </Dialog>
        <Dialog>
            <text>From people who duelled me before?</text>
            <source>Enemy</source>
            <phase>Beginning</phase>
            <sequential>false</sequential>
        </Dialog>
        <Dialog>
            <text>That's because dead men tell no tales</text>
            <source>Enemy</source>
            <phase>Beginning</phase>
            <sequential>false</sequential>
        </Dialog>
        
        <Dialog>
            <text>No more tales for you, young punk</text>
            <source>Enemy</source>
            <phase>Defeat</phase>
            <sequential>false</sequential>
        </Dialog>
        
      </Dialogs>
    </Enemy>

    <Enemy characterName="MaskedBandit">
      <health>100</health>
      <damage>5</damage>
      <prefab>MaskedBandit</prefab>
      <minTimeToClick>0.25</minTimeToClick>
      <maxTimeToClick>0.55</maxTimeToClick>
      <Dialogs>
        <Dialog>
            <text>Hand over your money</text>
            <source>Enemy</source>
            <phase>Beginning</phase>
            <sequential>true</sequential>
        </Dialog>
        <Dialog>
            <text>Or your life!</text>
            <source>Enemy</source>
            <phase>Beginning</phase>
            <sequential>false</sequential>
        </Dialog>

        <Dialog>
            <text>Nothing like the smell</text>
            <source>Enemy</source>
            <phase>Defeat</phase>
            <sequential>true</sequential>
        </Dialog>
        <Dialog>
            <text>Of blood and powder</text>
            <source>Enemy</source>
            <phase>Defeat</phase>
            <sequential>true</sequential>
        </Dialog>
        <Dialog>
            <text>In the morning</text>
            <source>Enemy</source>
            <phase>Defeat</phase>
            <sequential>false</sequential>
        </Dialog>
      </Dialogs>
    </Enemy>
	
    <Enemy characterName="BadDead">
      <health>100</health>
      <damage>5</damage>
      <prefab>BadDead</prefab>
      <minTimeToClick>0.2</minTimeToClick>
      <maxTimeToClick>0.5</maxTimeToClick>
