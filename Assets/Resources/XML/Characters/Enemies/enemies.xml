            <phase>Beginning</phase>
            <sequential>false</sequential>
        </Dialog>
        <Dialog>
            <text>Let's see how manly you'll be</text>
            <source>Enemy</source>
            <phase>Beginning</phase>
            <sequential>true</sequential>
        </Dialog>
        <Dialog>
            <text>Dying on the ground</text>
            <source>Enemy</source>
            <phase>Beginning</phase>
            <sequential>false</sequential>
        </Dialog>
        
        <Dialog>
