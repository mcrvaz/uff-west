            <phase>Beginning</phase>
            <sequential>false</sequential>
        </Dialog>
        <Dialog>
            <text>How's that?</text>
            <source>Enemy</source>
            <phase>Beginning</phase>
            <sequential>false</sequential>
        </Dialog>
        <Dialog>
            <text>Ha!</text>
            <source>Enemy</source>
            <phase>Beginning</phase>
            <sequential>true</sequential>
        </Dialog>
        <Dialog>
            <text>Really funny</text>
            <source>Enemy</source>
            <phase>Beginning</phase>
            <sequential>false</sequential>
        </Dialog>

        <Dialog>
